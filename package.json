{
  "name": "alpaca-market-cli",
  "version": "0.0.0",
  "description": "Alpaca Market Data CLI",
  "author": "Jason Ives @jivebot",
  "bin": {
    "alpaca": "./bin/run"
  },
  "homepage": "https://github.com/jivebot/alpaca-market-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "jivebot/alpaca-market-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@fast-csv/format": "^4.3.5",
    "@master-chief/alpaca": "^6.3.17",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.12",
    "dotenv": "^16.0.0",
    "luxon": "^2.3.2"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/luxon": "^2.3.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "chai": "^4",
    "eslint": "^8.13.0",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "oclif": {
    "bin": "alpaca",
    "dirname": "alpaca",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "bars": {
        "description": "Download bar data"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc.js",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/jivebot/alpaca-market-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
